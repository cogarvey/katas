const sumLargestNumbers = function(data) {
  let nums = data.sort();
  return nums[data.length - 1] + nums[data.length - 2];
};

console.log(sumLargestNumbers([1, 10]));
console.log(sumLargestNumbers([1, 2, 3]));
console.log(sumLargestNumbers([10, 4, 34, 6, 92, 2]));


// EXPECTED OUTPUT:
// 11
// 5
// 126